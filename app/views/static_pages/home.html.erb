<section id="contact" class="section section-contact">
	<div class="container">
		<div class="row text-center">
			<h1 class="title">Contact us</h1>
		</div>
		<div class="row">

			<div class="col-md-6 col-centered">
				<div class="row parent col-centered">
					<div id="select-start" class="col-md-6 child">
						<div class="text-center">
							<img class="img-responsive center-block" src="<%= asset_path('paper-plane.png') %>" alt="Start your Project">
							<h4>Start your Project</h4>
							<img id="line-start" class="center-block" src="<%= asset_path('line-selected.png') %>" alt="">
						</div>
					</div>
					<div id="select-join" class="col-md-6 child">
						<div class="text-center">
							<img class="img-responsive center-block" src="<%= asset_path('paper.png') %>" alt="Join our team">
							<h4>Join our team</h4>
							<img id="line-join" class="center-block" src="<%= asset_path('line-unselect.png') %>" alt="">
						</div>
					</div>
				</div>
			</div>

		</div>
		<div class="row">

			<div class="col-md-6 col-centered">
				<div class="row text-center">
					<div class="col-md-6">
						<input type="text" class="form-control" id="input-name" placeholder="Name">
					</div>
					<div class="col-md-6">
						<input type="email" class="form-control" id="input-email" placeholder="Email">
					</div>
					<div class="col-md-6">
						<input type="text" class="form-control" id="input-company" placeholder="Company name">
					</div>
					<div class="col-md-6">
						<input type="text" class="form-control" id="input-industry" placeholder="Industry">
					</div>
					<div class="col-md-12">
						<textarea class="form-control" rows="3" placeholder="More details about your project"></textarea>
					</div>
					<div class="col-md-12">
						<button type="button" class="btn btn-primary">Send</button>
					</div>
				</div>
			</div>

		</div>
	</div>
</section>

