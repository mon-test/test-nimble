<section id="home" class="section section-home">
	<div class="container">
			<img class="img-responsive center-block" src="<%= asset_path('nimble-logo.png') %>" alt="nimble logo">
			<div class="text-center">
				<h3 class="sub-title">We Build BEAUTIFUL Apps<br>With STRONG Cores</h3>
			</div>
			<div class="row text-center">
				<div class="social-wrapper">
					<img class="" src="<%= asset_path('facebook.png') %>" alt="Facebook">
					<img class="" src="<%= asset_path('twitter.png') %>" alt="Twitter">
					<img class="" src="<%= asset_path('github.png') %>" alt="Github">
				</div>
				<h4 class="follow">Follow us</h4>
			</div>
			<div class="row text-center">
				<div class="paper-plane-wrapper">
					<a href="#contact">
						<img class="img-responsive center-block" src="<%= asset_path('paper-plane.png') %>" alt="Start your Project">
						<h4>Start your Project</h4>
